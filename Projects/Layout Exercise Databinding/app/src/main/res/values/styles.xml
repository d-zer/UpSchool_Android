<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="EntryTitleStyle">
        <item name="android:fontFamily">@font/acme</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#2C2B47</item>
        <item name="android:textSize">@dimen/entry_title_size</item>
    </style>

    <style name="HomeSubTitleStyle">
        <item name="android:fontFamily">@font/acme</item>
        <item name="android:textColor">#B9B8C6</item>
        <item name="android:textSize">17sp</item>
    </style>

    <style name="StartInputStyle">
        <item name="android:background">@drawable/ic_start_input_background</item>
        <item name="android:fontFamily">@font/acme</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:textColorHint">#2C2B47</item>
    </style>

    <style name="infoTime">
        <item name="android:fontFamily">@font/acme</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>

    <style name="price">
        <item name="android:textColor">#2C2B47</item>
        <item name="android:textSize">24sp</item>
        <item name="android:fontFamily">@font/acme</item>
    </style>

    <style name="per">
        <item name="android:textColor">#B9B8C6</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="perPrice">
        <item name="android:textColor">#87869E</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="buttonText">
        <item name="android:textColor">#FFF1EF</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:fontFamily">@font/acme</item>
    </style>
</resources>